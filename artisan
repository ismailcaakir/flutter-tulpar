#!/usr/bin/env bash
tulparDir="$( cd -- "$(dirname "$0/")" >/dev/null 2>&1 ; pwd -P )"
projectDir="$( cd -- "$(dirname "$tulparDir/../../")" >/dev/null 2>&1 ; pwd -P )"
libFolder="$projectDir/lib"

## RENAME APP
app:install() {
    sh "$tulparDir/bin/app-install.sh" $tulparDir $projectDir $libFolder
}

"$@"